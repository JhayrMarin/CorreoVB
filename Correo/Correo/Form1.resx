  <data name="txtConfirmacion.Text" xml:space="preserve">
    <value>&lt;!DOCTYPE html&gt;
&lt;html&gt;
&lt;head&gt;
&lt;style&gt;
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 40%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
&lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;

&lt;center&gt;&lt;h2&gt;Detalle de Correos Enviados.&lt;/h2&gt;&lt;/center&gt;&lt;

&lt;center&gt;&lt;table&gt;
  &lt;tr&gt;
    &lt;th&gt;Nombre&lt;/th&gt;
    &lt;th&gt;Correo&lt;/th&gt;
  &lt;/tr&gt;
  @Datos 
&lt;/table&gt;&lt;/center&gt;

&lt;/body&gt;
&lt;/html&gt;</value>
